{
  "name": "DatingProfileOptimizer",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "lint:security": "eslint . --ext .ts,.tsx --config .eslintrc.security.js",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:security": "jest --config jest.security.config.js",
    "test:performance": "jest --testPathPattern=performance --testTimeout=60000",
    "test:integration": "jest --testPathPattern=integration --testTimeout=30000",
    "test:e2e": "detox test",
    "test:e2e:build": "detox build --configuration ios.sim.debug",
    "test:all": "npm run test:ci && npm run test:security && npm run test:performance && npm run test:integration",
    "audit:dependencies": "npm audit --audit-level moderate",
    "audit:security": "npm run lint:security && npm run test:security",
    "quality:check": "npm run lint && npm run test:all && npm run audit:security",
    "pre-commit": "npm run quality:check",
    "build:android:release": "cd android && ./gradlew assembleRelease",
    "build:ios:release": "cd ios && xcodebuild -workspace DatingProfileOptimizer.xcworkspace -scheme DatingProfileOptimizer -configuration Release",
    "release:check": "npm run quality:check && npm run build:android:release"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.4.5",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.5",
    "@stripe/stripe-react-native": "^0.50.3",
    "mixpanel-react-native": "^3.1.2",
    "@amplitude/analytics-react-native": "^1.4.8",
    "@segment/analytics-react-native": "^2.20.4",
    "@datadog/mobile-react-native": "^2.8.1",
    "react-native-performance": "^5.1.2",
    "react-native-device-info": "^10.13.0",
    "react-native-network-info": "^5.2.1",
    "react-native-localize": "^3.0.6",
    "react-native-exception-handler": "^2.10.10",
    "openai": "^5.12.2",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-image-crop-picker": "^0.51.0",
    "react-native-image-picker": "^8.2.1",
    "react-native-material-design": "^0.3.7",
    "react-native-paper": "^5.14.5",
    "react-native-safe-area-context": "^5.6.0",
    "react-native-screens": "^4.13.1",
    "react-native-vector-icons": "^10.3.0",
    "react-native-keychain": "^8.2.0",
    "react-native-device-info": "^10.13.0",
    "react-native-biometrics": "^3.0.1",
    "react-native-ssl-pinning": "^1.5.1",
    "react-native-aes-crypto": "^2.1.1",
    "@react-native-firebase/auth": "^20.4.0",
    "@react-native-firebase/firestore": "^20.4.0",
    "@react-native-firebase/analytics": "^20.4.0",
    "react-native-permissions": "^4.1.5",
    "crypto-js": "^4.2.0",
    "jsonwebtoken": "^9.0.2",
    "validator": "^13.12.0",
    "bcryptjs": "^2.4.3",
    "speakeasy": "^2.0.0",
    "qrcode": "^1.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "@types/crypto-js": "^4.2.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/validator": "^13.12.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/speakeasy": "^2.0.10",
    "@types/qrcode": "^1.5.5",
    "@testing-library/react-native": "^12.7.2",
    "@testing-library/jest-native": "^5.4.3",
    "babel-jest": "^29.2.1",
    "detox": "^20.26.2",
    "eslint": "^8.19.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-testing-library": "^6.4.0",
    "jest": "^29.2.1",
    "jest-circus": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-html-reporters": "^3.1.7",
    "jest-security-audit": "^1.0.0",
    "jest-performance-testing": "^1.0.0",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4",
    "sonarjs": "^1.0.3",
    "supertest": "^7.0.0",
    "nock": "^14.0.0",
    "msw": "^2.4.9",
    "@types/supertest": "^6.0.2",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10"
  },
  "engines": {
    "node": ">=18"
  },
  "overrides": {
    "semver": "^7.5.4",
    "ws": "^8.18.0",
    "follow-redirects": "^1.15.4"
  }
}
